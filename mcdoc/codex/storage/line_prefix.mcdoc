use ::java::util::text::Text
use super::CommandStorage

type BulletType = #[dispatcher_key="codex:bullet_type"] string

dispatch :storage[codex:bullets] to CommandStorage<struct BulletMap {
  /// Prefix text of description lines
  [BulletType]: Text,
}>

dispatch codex:bullet_type[
  none,
  dot,
  section_header,
  section_content,
  section_content_continued,
  section_last_content,
  indent
] to any
